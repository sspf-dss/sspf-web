  "version": "0.2.0",
  "configurations": [
    {
      "name": "sspf-web Chrome Debugger",
      "type": "chrome",
      "request": "launch",
      "url": "http://localhost:4200",
      "webRoot": "${workspaceFolder}/apps/sspf-public/src"
    },
    {
      "name": "sspf-admin Chrome Debugger",
      "type": "chrome",
      "request": "launch",
      "url": "http://localhost:4201",
      "webRoot": "${workspaceFolder}/apps/sspf-admin/src"
    },
    {
      "command": "nx serve sspf-web",
      "name": "Run sspf-web",
      "cwd": "${workspaceFolder}",
      "request": "launch",
      "type": "node-terminal"
    },
    {
      "command": "nx serve sspf-admin",
      "name": "Run sspf-admin",
      "cwd": "${workspaceFolder}",
      "request": "launch",
      "type": "node-terminal"
    }

