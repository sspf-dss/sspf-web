{
  "name": "sspf-web",
  "version": "0.3.2",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "commit": "cz",
    "release": "standard-version",
    "release:patch": "standard-version --release-as patch",
    "release:minor": "standard-version --release-as minor",
    "release:major": "standard-version --release-as major",
    "postrelease": "git push --follow-tags origin main && npm run publish-if-needed",
    "publish-if-needed": "echo 'skip npm publish for Angular app'"
  },
  "private": true,
  "dependencies": {
    "@angular/cdk": "^20.0.2",
    "@angular/common": "^20.0.1",
    "@angular/compiler": "^20.0.1",
    "@angular/core": "^20.0.1",
    "@angular/forms": "^20.0.1",
    "@angular/material": "^20.0.2",
    "@angular/platform-browser": "^20.0.1",
    "@angular/platform-browser-dynamic": "^20.0.1",
    "@angular/router": "^20.0.1",
    "@ngrx/signals": "^19.2.0",
    "@strapi/client": "^1.4.0",
    "@tailwindcss/postcss": "^4.1.7",
    "keycloak-angular": "^20.0.0",
    "keycloak-js": "^26.2.0",
    "lodash-es": "^4.17.21",
    "marked": "^15.0.12",
    "ngx-extended-pdf-viewer": "^23.3.1",
    "ngx-markdown": "^20.0.0",
    "postcss": "^8.5.3",
    "qs": "^6.14.0",
    "rxjs": "~7.8.0",
    "strapi-sdk-js": "^3.0.0",
    "tailwindcss": "^4.1.7",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular/build": "^20.0.1",
    "@angular/cli": "^20.0.1",
    "@angular/compiler-cli": "^20.0.1",
    "@angular/localize": "^20.0.1",
    "@types/jasmine": "~5.1.0",
    "@types/lodash-es": "^4.17.12",
    "commitizen": "^4.3.1",
    "cz-conventional-changelog": "^3.3.0",
    "esbuild-visualizer": "^0.7.0",
    "jasmine-core": "~5.6.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "ng-packagr": "^20.0.0",
    "postcss": "^8.5.3",
    "standard-version": "^9.5.0",
    "typescript": "~5.8.3"
  },
  "overrides": {
    "@ngrx/signals": {
      "@angular/common": "$@angular/common",
      "@angular/core": "$@angular/core"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
